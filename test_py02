import random
player = input("请输入石头、剪刀、布：\n")
computer = random.choice(["剪刀","石头","布"])
print(computer)
if (player == "石头" and computer == "剪刀") or (player == "剪刀" and computer == "布") or (player == "布" and computer == "石头"):
    print("玩家获胜")
elif (player == computer):
    print("平局")
else:
    print("玩家输了")